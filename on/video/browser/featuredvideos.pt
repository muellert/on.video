<div class="portlet portletFeaturedVideo">

    <h1 class="portletHeader">
        <span class = "tile" tal:content="view/data/header" />
    </h1>

    <div class="featuredVideos">
      <div class="portletItem" tal:repeat="vid view/viewthumb">
       <a href="#" tal:attributes="href vid/url" class="fvideo">
	 <div tal:content="vid/title" />

	 <img src="#" width="100" height="90"
	      alt="#" title="#"
	      tal:attributes="src vid/thumbnail;
	                      alt vid/title;
	                      title vid/title"
	  />
	  <br />	
       </a>
     </div>
   </div>
</div>
